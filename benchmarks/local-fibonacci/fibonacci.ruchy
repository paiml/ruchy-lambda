// Fibonacci recursive (n=35) - Ruchy
// Matches AWS Lambda handler implementation
// Expected result: 9227465

pub fun fibonacci(n: i32) -> i32 {
    if n <= 1 {
        n
    } else {
        fibonacci(n - 1) + fibonacci(n - 2)
    }
}

